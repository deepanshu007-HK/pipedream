
#include <iostream>

using namespace std;

void add(int a[],int n,int x){
       int pos=0;
   cin>>pos;
   int ind=pos-1;
   int i=0;
   for( i=0;i<n;i++){
       if(i==ind){
           break;
       }
   }
   a[i]=x;
   for(int j=0;j<n;j++){
       cout<<a[j]<<" ";
   }
   
}
void sub(int a[],int n,int x){
           int pos=0;
   cin>>pos;
   int ind=pos-1;
   int i=0;
   for( i=0;i<n;i++){
       if(i==ind){
           break;
       }
   }
   int j=0;
   for(j=i;j<n;j++){
      a[j]=a[j+1];
      
   }
   a[n-1]=0;
   for(int k=0;k<n;k++){
       cout<<a[k]<<" ";
   }
}



int main()
{
   int n;
   cin>>n;
   int a[n];
   for(int i=0;i<n;i++){
       cin>>a[i];
   }
   int x;
   cin>>x;
   
add(a,n,x);

sub(a,n,x);

    return 0;
}
